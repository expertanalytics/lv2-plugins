@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .

<https://github.com/expertanalytics/lv2-plugins/tree/master/foo>
    a lv2:Plugin ,
        lv2:DelayPlugin ;

        lv2:project <https://github.com/expertanalytics/lv2-plugins/tree/master/foo> ;

        doap:name "Foo", "Foo"@en-gb;
        doap:license <http://opensource.org/licenses/isc> ;
        lv2:port [
                    # Input audio port
                    a lv2:AudioPort ,
                    lv2:InputPort ;
                    lv2:index 0 ;
                    lv2:symbol "in" ;
                    lv2:name "In"
                    ] , [
                    # Output Audio Port
                    a lv2:AudioPort ,
                    lv2:OutputPort ;
                    lv2:index 1 ;
                    lv2:symbol "out" ;
                    lv2:name "Out"
                    ] .
