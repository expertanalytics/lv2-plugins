@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .

<https://github.com/expertanalytics/lv2-plugins/tree/master/simple_delay>
    a lv2:Plugin ,
        lv2:DelayPlugin ;

        lv2:project <https://github.com/expertanalytics/lv2-plugins/tree/master/simple_delay> ;

        doap:name "Simple delay" ,
                    "Simple delay"@en-gb;
        doap:license <http://opensource.org/licenses/isc> ;
        lv2:port [
                   # Input audio port
                    a lv2:AudioPort ,
                    lv2:InputPort ;
                    lv2:index 0 ;
                    lv2:symbol "in" ;
                    lv2:name "In"
                    ] , [
                    # Output Audio Port
                    a lv2:AudioPort ,
                    lv2:OutputPort ;
                    lv2:index 1 ;
                    lv2:symbol "out" ;
                    lv2:name "Out"
                    ] , [
                    # delay time port
                    a lv2:InputPort ,
                    lv2:ControlPort ;
                    lv2:index 2 ;
                    lv2:symbol "s" ;
                    lv2:name "Delay time";
                    lv2:default 20.0 ;
                    lv2:minimum 1.0 ;
                    lv2:maximum 8000.0 ;
                    lv2:portProperty pprops:logarithmic ;
                    units:unit units:ms ;
                     ] , [
                    # feedback port
                    a lv2:InputPort ,
                    lv2:ControlPort ;
                    lv2:index 3 ;
                    lv2:symbol "gain" ;
                    lv2:name "Feedback";
                    lv2:default 0.0 ;
                    lv2:minimum 0.0 ;
                    lv2:maximum 100.0 ;
                    units:unit units:pct ;
                    ], [
                     # dry / wet amount port
                     a lv2:InputPort ,
                     lv2:ControlPort ;
                     lv2:index 4 ;
                     lv2:symbol "gain" ;
                     lv2:name "Dry Wet Amount";
                     lv2:default 40 ;
                     lv2:minimum 0.0 ;
                     lv2:maximum 100.0 ;
                     units:unit units:pct ;
                      ], [
                    # output gain
                    a lv2:InputPort ,
                    lv2:ControlPort ;
                    lv2:index 5 ;
                    lv2:symbol "gain" ;
                    lv2:name "Output gain";
                    lv2:default 0.0 ;
                    lv2:minimum -90.0 ;
                    lv2:maximum 12.0 ;
                    units:unit units:db ;
                    ].
